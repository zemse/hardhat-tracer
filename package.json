    "src/",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@types/chai": "^4.3.0",
    "@types/debug": "^4.1.7",
    "@types/fs-extra": "^5.0.4",
    "@types/mocha": "^5.2.6",
    "@types/node": "^16.0.0",
    "@types/readable-stream": "^2.3.14",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "hardhat": "^2.12.2",
    "hardhat-deploy": "^0.11.25",
    "mocha": "^7.1.2",
    "prettier": "2.0.5",
    "ts-node": "^10.7.0",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
